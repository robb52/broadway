<div class="col-md-10 offset-md-1">
  <div class="edit-review-form">
    <h1> Edit Review </h1>
    <%= simple_form_for([@play, @review]) do |f| %>
      <div class="rating-form">
        <%= f.input :rating %>
      </div>
        <%= f.input :comment %>
        <%= f.button :submit, class: "btn btn-primary1" %>
    <% end %>
  </div>
</div>